{
  "name": "webdriverio-test",
  "version": "0.0.1",
  "description": "A basic example of automation with WebdriverIO, Cucumberjs & TypeScript.",
  "scripts": {
    "test-local": "node ./node_modules/typescript/lib/tsc.js && node ./node_modules/webdriverio/bin/wdio ./conf/local.conf.js && node ./node_modules/rimraf/bin.js tsrc",
    "test-sauce": "node ./node_modules/typescript/lib/tsc.js && node ./node_modules/webdriverio/bin/wdio ./conf/saucelabs.conf.js && node ./node_modules/rimraf/bin.js tsrc"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.32",
    "@types/webdriverio": "^4.4.0",
    "allure-commandline": "1.4.23",
    "chai": "^3.5.0",
    "cucumber": "^1.3.1",
    "eslint": "~3.13.1",
    "eslint-config-airbnb-base": "~11.0.1",
    "jest": "~18.1.0",
    "ts-node": "^3.0.2",
    "typescript": "^2.0.6",
    "typescript-node": "^0.1.3",
    "wdio-cucumber-framework": "~0.2.15",
    "wdio-cucumber-reporter": "~0.0.x",
    "wdio-junit-reporter": "^0.4.3",
    "wdio-sauce-service": "^0.4.0",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "~0.0.5",
    "webdriverio": "^4.5.0",

  }
}
